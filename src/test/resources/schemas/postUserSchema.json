{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "name",
    "email"
  ],
  "properties": {
    "id": {
      "type": "string",
      "default": "",
      "title": "The id Schema",
      "examples": [
        "e82bba16-b641-41d5-8ff6-f8b82bf87201"
      ]
    },
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "Teste"
      ]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": [
        "user23@teste.com"
      ]
    }
  },
  "examples": [{
    "id": "e82bba16-b641-41d5-8ff6-f8b82bf87201",
    "name": "Teste",
    "email": "user23@teste.com"
  }]
}